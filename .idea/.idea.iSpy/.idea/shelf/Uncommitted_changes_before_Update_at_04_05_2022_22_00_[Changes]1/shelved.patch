+XmlSchemaElement GetRefElement (XmlSchemaElement elem)
+{
+	if (!elem.RefName.IsEmpty)
+		return (XmlSchemaElement) schemas.Find (elem.RefName, typeof(XmlSchemaElement));
+	else
+		return elem;
+}
