+		string BuildQueryString (OperationMessage opm)
+		{
+			string s = "";
+			Message msg = descriptions.GetMessage (opm.Message);
+			foreach (MessagePart part in msg.Parts)
+			{
+				if (s.Length != 0) s += "&";
+				s += part.Name + "=" + GetLiteral (part.Type.Name);
+			}
+			return s;
+		}
