+	re = /\t/g;
+	text = text.replace (re, "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
+	
+	re = /\<\s*(\/?)\s*(.*?)\s*(\/?)\s*\>/g;
+	text = text.replace (re,"{blue:&lt;$1}{maroon:$2}{blue:$3&gt;}");
