+	re = /{(\w*):(.*?)}/g;
+	text = text.replace (re,"<span style='color:$1'>$2</span>");
+
+	re = /"(.*?)"/g;
+	text = text.replace (re,"\"<span style='color:purple'>$1</span>\"");
+
+	re = /\r\n|\r|\n/g;
+	text = text.replace (re, "<br/>");
+	
+	return text;
+}
