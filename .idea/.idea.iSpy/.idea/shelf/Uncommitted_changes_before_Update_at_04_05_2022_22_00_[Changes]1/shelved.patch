+	var post_data = null;
+	req = getReq ();
+	req.onreadystatechange = stateChange;
+	if (command == "GET") {
+		url = url + "?" + qs;
+	} else {
+		post_data = qs;
+	}
+	req.open (command, url,  true); 
+	if (command == "POST")
+		req.setRequestHeader ("Content-Type", "application/x-www-form-urlencoded");
+	req.send (post_data); 
+}
+
+function stateChange () {
